{
  "name": "apiclient",
  "version": "0.0.0",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "type": "module",
  "scripts": {
    "start:dev": "npm run watch",
    "build": "tsc -p ./tsconfig.build.json",
    "watch": "nodemon -q --watch ../../app/backend/src -e ts -x \"npm run build\"",
    "generate": "orval --config ./orval.config.ts",
    "prebuild": "./scripts/extract.ts && npm run generate"
  },
  "devDependencies": {
    "orval": "^6.24.0"
  },
  "dependencies": {
    "axios": "^1.6.7"
  }
}
